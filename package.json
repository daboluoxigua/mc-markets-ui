{
  "name": "@mc-markets/ui",
  "private": false,
  "version": "1.0.37",
  "type": "module",
  "author": "chenyu",
  "description": "Vue 3 组件库，基于 Element Plus 的自定义组件",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./style": "./dist/style.css",
    "./styles": "./dist/style.css",
    "./dist/style.css": "./dist/style.css",
    "./scss-variables": "./packages/styles/variables.scss",
    "./styles/index": "./packages/styles/index.scss",
    "./theme-config": "./packages/styles/theme-config.js",
    "./element-plus": {
      "import": "./node_modules/element-plus/dist/index.full.mjs",
      "require": "./node_modules/element-plus/dist/index.full.js"
    },
    "./element-plus/style": "./node_modules/element-plus/dist/index.css",
    "./element-plus/styles": "./node_modules/element-plus/dist/index.css"
  },
  "files": [
    "dist",
    "packages/styles",
    "README.md",
    "node_modules/element-plus/dist"
  ],
  "keywords": [
    "vue3",
    "javascript",
    "ui",
    "component",
    "element-plus",
    "ui-library",
    "components"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:inspector": "cross-env VUE_INSPECTOR=true vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "publish:lib": "node publish.js"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "element-plus": "^2.11.2",
    "highlight.js": "^11.11.1",
    "vue": "^3.3.0",
    "vue-router": "^4.5.1"
  },
  "peerDependencies": {
    "element-plus": "^2.11.2",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/daboluoxigua/npm-mc-markets-ui.git"
  },
  "bugs": {
    "url": "https://gitee.com/daboluoxigua/npm-mc-markets-ui/issues"
  },
  "homepage": "https://gitee.com/daboluoxigua/npm-mc-markets-ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@iconify-json/ep": "^1.1.10",
    "@iconify/vue": "^4.1.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.1",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.92.1",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^4.6.0",
    "stylelint-stylistic": "^0.4.0",
    "svgo": "^3.0.2",
    "terser": "^5.16.9",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^7.1.5",
    "vite-plugin-vue-inspector": "^3.4.0"
  }
}

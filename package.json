{
  "name": "@mc-markets/ui",
  "private": false,
  "version": "1.0.77",
  "type": "module",
  "author": "chenyu",
  "description": "Vue 3 组件库，基于 Element Plus 的自定义组件",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles/index": "./packages/styles/index.scss",
    "./styles/font": "./packages/styles/font/iconfont.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "packages/styles",
    "packages/components",
    "packages/hooks",
    "packages/utils",
    "README.md"
  ],
  "keywords": [
    "vue3",
    "javascript",
    "ui",
    "component",
    "element-plus",
    "ui-library",
    "components"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite --config vite.config.demo.js",
    "build": "node scripts/build.js",
    "build:clean": "rimraf dist",
    "build:demo": "vite build --config vite.config.demo.js",
    "preview": "vite preview",
    "preview:demo": "vite preview --config vite.config.demo.js --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "publish:lib": "node publish.js"
  },
  "dependencies": {
    "element-plus": "^2.11.2",
    "highlight.js": "^11.11.1",
    "vue": "^3.3.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "sass": "^1.92.1",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vue-inspector": "^4.0.0",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "element-plus": "^2.11.2",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daboluoxigua/mc-markets-ui.git"
  },
  "bugs": {
    "url": "https://github.com/daboluoxigua/mc-markets-ui/issues"
  },
  "homepage": "https://daboluoxigua.github.io/mc-markets-ui/",
  "publishConfig": {
    "access": "public"
  }
}

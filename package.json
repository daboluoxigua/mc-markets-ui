{
  "name": "@mc-markets/ui",
  "private": false,
  "version": "1.0.37",
  "type": "module",
  "author": "chenyu",
  "description": "Element Plus 完整导出包，提供所有组件、指令和服务",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style": "./dist/style.css",
    "./styles": "./dist/style.css",
    "./dist/style.css": "./dist/style.css",
    "./scss-variables": "./packages/styles/variables.scss",
    "./styles/index": "./packages/styles/index.scss",
    "./theme-config": "./packages/styles/theme-config.ts",
    "./element-plus": {
      "import": "./node_modules/element-plus/dist/index.full.mjs",
      "require": "./node_modules/element-plus/dist/index.full.js",
      "types": "./node_modules/element-plus/dist/index.d.ts"
    },
    "./element-plus/style": "./node_modules/element-plus/dist/index.css",
    "./element-plus/styles": "./node_modules/element-plus/dist/index.css",
    "./locales": "./dist/locales/index.js",
    "./themes": "./dist/themes/index.js"
  },
  "files": [
    "dist",
    "packages/styles",
    "README.md",
    "USAGE.md",
    "node_modules/element-plus/dist"
  ],
  "keywords": [
    "vue3",
    "typescript",
    "ui",
    "component",
    "element-plus",
    "element-ui",
    "ui-library",
    "components"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "dev:fast": "cross-env NODE_ENV=development vite --force",
    "dev:inspector": "cross-env VUE_INSPECTOR=true vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "publish:lib": "node publish.js"
  },
  "dependencies": {
    "@vueuse/core": "^10.5.0",
    "element-plus": "^2.8.0",
    "vue": "^3.3.0"
  },
  "peerDependencies": {
    "element-plus": "^2.8.0",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/daboluoxigua/npm-mc-markets-ui.git"
  },
  "bugs": {
    "url": "https://gitee.com/daboluoxigua/npm-mc-markets-ui/issues"
  },
  "homepage": "https://gitee.com/daboluoxigua/npm-mc-markets-ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@iconify-json/ep": "^1.1.10",
    "@iconify/vue": "^4.1.1",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "bumpp": "^9.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.38.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "http-server": "^14.1.1",
    "lint-staged": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.77.0",
    "simple-git-hooks": "^2.8.1",
    "sortablejs": "^1.15.0",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^4.6.0",
    "stylelint-stylistic": "^0.4.0",
    "svgo": "^3.0.2",
    "terser": "^5.16.9",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.2.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-spritesmith": "^0.1.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-inspector": "^3.4.0",
    "vite-plugin-vue-meta-layouts": "^0.2.2",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.2.0"
  }
}

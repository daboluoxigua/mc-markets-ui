{
  "name": "@mc-markets/ui",
  "private": false,
  "version": "1.1.70",
  "type": "module",
  "author": "chenyu",
  "description": "Vue 3 组件库，基于 Element Plus 的自定义组件",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./resolver": {
      "import": "./packages/resolver.js",
      "require": "./packages/resolver.cjs",
      "types": "./packages/resolver.d.ts"
    },
    "./styles/index": "./packages/styles/index.scss",
    "./styles/index.scss": "./packages/styles/index.scss",
    "./styles/font": "./packages/styles/font/iconfont.css",
    "./styles/colorfont": "./packages/styles/colorfont/iconfont.css",
    "./dist/style": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "packages/styles",
    "packages/components",
    "packages/hooks",
    "packages/utils",
    "packages/resolver.js",
    "packages/resolver.cjs",
    "packages/resolver.d.ts",
    "README.md",
    "RESOLVER_USAGE.md",
    "STYLE_IMPORT_GUIDE.md",
    "QUICK_FIX_STYLE.md",
    "CLEAR_VITE_CACHE.md",
    "FIX_EXPORT_ISSUE.md",
    "TEST_RESOLVER.md",
    "USAGE_GUIDE.md",
    "QUICK_START.md",
    "FIX_ESM_ERROR.md",
    "TROUBLESHOOTING.md",
    "ELEMENT_PLUS_ERROR_FIX.md",
    "vite.config.example.js"
  ],
  "keywords": [
    "vue3",
    "vue",
    "element-plus",
    "ui-library",
    "components",
    "auto-import",
    "unplugin",
    "unplugin-vue-components",
    "unplugin-auto-import",
    "vite",
    "webpack",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite --config vite.config.demo.js",
    "dev:h5": "cross-env VITE_H5=1 vite --config vite.config.demo.js",
    "build": "node scripts/build.js",
    "build:clean": "rimraf dist",
    "build:demo": "vite build --config vite.config.demo.js",
    "build:demo:h5": "cross-env VITE_H5=1 vite build --config vite.config.demo.js",
    "preview": "vite preview",
    "preview:demo": "vite preview --config vite.config.demo.js --port 4173",
    "preview:demo:h5": "cross-env VITE_H5=1 vite preview --config vite.config.demo.js --port 4174",
    "lint": "eslint . --ext .vue,.js,.jsx --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "publish:lib": "node publish.js"
  },
  "dependencies": {
    "@mc-markets/ui": "^1.1.19",
    "@vue/compiler-core": "^3.5.24",
    "element-plus": "^2.11.2",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.0",
    "postcss-pxtorem": "^6.1.0",
    "vant": "^4.9.21",
    "vue": "^3.3.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vant/auto-import-resolver": "^1.3.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "sass": "^1.92.1",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vue-inspector": "^4.0.0",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "element-plus": "^2.11.2",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daboluoxigua/mc-markets-ui.git"
  },
  "bugs": {
    "url": "https://github.com/daboluoxigua/mc-markets-ui/issues"
  },
  "homepage": "https://daboluoxigua.github.io/mc-markets-ui/",
  "publishConfig": {
    "access": "public"
  }
}
